{
  "name": "clean_architecture_tdd",
  "version": "1.0.0",
  "description": "",
  "main": "src/main/index.js",
  "scripts": {
    "postinstall": "husky install",
    "start": "node -r esm src/main/index.js",
    "lint-staged": "npx lint-staged",
    "test": "jest --noStackTrace --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit.config.js",
    "test:integration": "npm test -- --watch -c jest-integration.config.js",
    "test:staged": "npm test -- --findRelatedTests --silent",
    "test:ci": "npm test -- --coverage --silent"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aperezl/clean_architecture_tdd.git"
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "npm run test:staged"
    ]
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aperezl/clean_architecture_tdd/issues"
  },
  "homepage": "https://github.com/aperezl/clean_architecture_tdd#readme",
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "babel-jest": "^26.6.3",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "standard": "^16.0.3",
    "supertest": "^6.1.3"
  },
  "dependencies": {
    "@shelf/jest-mongodb": "^1.2.3",
    "bcrypt": "^5.0.0",
    "esm": "^3.2.25",
